{
    "auth": {
      "username": "admin",
      "sessionDuration": 3600000,
      "maxFailedAttempts": 5,
      "lockoutDuration": 900000
    },
    "ui": {
      "refreshInterval": 5000,
      "maxLogEntries": 100,
      "showDetailedLogs": false,
      "enableRemoteControl": true,
      "theme": "light"
    },
    "audit": {
      "logActions": true,
      "logAdminActions": true,
      "logRetentionDays": 30
    },
    "security": {
      "allowRemoteAccess": false,
      "requireHTTPS": false,
      "sessionIdleTimeout": 1800000,
      "enforceIpMatch": true
    },
    "features": {
      "userManagement": true,
      "messageModeration": true,
      "systemMetrics": true,
      "logExport": true
    }
  }